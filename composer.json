{
  "name": "bluora/php-slack-api",
  "description": "Access your Slack Team's API through PHP objects",
  "homepage": "http://bluora.github.io/php-slack-api/",
  "keywords": [
    "slack",
    "slack api",
    "chat"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rocco Howard",
      "homepage": "https://github.com/bluora",
      "email": "rocco@hnh.digital"
    },
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "jms/serializer": "^1.0",
    "symfony/event-dispatcher": "^2.3|^3.0",
    "symfony/yaml": "^2.3|^3.0",
    "guzzlehttp/guzzle": "^6.0",
    "doctrine/collections": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "mockery/mockery": "^0.9",
    "symfony/var-dumper": "^2.3|^3.0"
  },
  "suggest": {
    "bluora/laravel-slack-api": "For easy integration with your Laravel projects"
  },
  "autoload": {
    "psr-4": {
      "CL\\Slack\\": "src/",
      "CL\\Slack\\Test\\": "tests/Test/",
      "CL\\Slack\\Tests\\": "tests/Tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
